{
  "name": "rcam-demo",
  "version": "0.0.0",
  "private": true,
  "description": "redux-cached-api-middleware demo",
  "license": "MIT",
  "scripts": {
    "build": "react-scripts build",
    "clean": "rimraf build",
    "format": "prettier --write '**/*.*' && sort-package-json",
    "lint": "eslint .",
    "qa": "CI=true run-p test lint",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "try-demo": "run-s clean build && serve -s build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-cached-api-middleware": "^0.2.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "^1.1.0",
    "autoprefixer": "^9.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^1.0.0-rc.13",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "serve": "^9.4.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn qa"
    }
  }
}
